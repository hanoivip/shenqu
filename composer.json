{
    "name": "hanoivip/shenqu",
    "description": "Shenqu Auth",
    "type": "library",
    "license": "MIT",
    "autoload" : {
		"psr-4" : {
			"Hanoivip\\Shenqu\\" : "src/"
		}
	},
	"repositories" : [{
			"type" : "path",
			"url" : "../events",
			"options" : {
				"symlink" : true
			}
		}
	],
	"require" : {
		"php" : ">=7.0.0",
		"guzzlehttp/guzzle" : "^6.3",
		"laravel/framework" : ">=5.5",
		"laravel/tinker" : "~1.0",
		"mervick/curl-helper" : "^1.1",
		"hanoivip/events" : "dev-master"
	},
	"extra" : {
		"laravel" : {
			"providers" : [
				"Hanoivip\\Shenqu\\AuthServiceProvider"
			]
		}
	}
}
